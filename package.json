    "unichat-ts": "1.0.10"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/amidabuddha/unichat-ts-mcp-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amidabuddha/unichat-ts-mcp-server.git"
  }
